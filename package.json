{
  "name": "inspectify-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "clean": "rimraf dist",
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "postbuild": "cp -r generated dist/ && tsc-alias",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev": "dotenv -e env/local.env -- nest start --watch",
    "start:debug": "dotenv -e env/local.env -- nest start --debug --watch",
    "start:prod": "dotenv -e env/production.env -- node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migrate:dev": "dotenv -e env/local.env -- prisma migrate dev --preview-feature",
    "migrate:prod": "dotenv -e env/production.env -- prisma migrate dev --preview-feature",
    "migrate:dev:create": "dotenv -e env/local.env -- prisma migrate dev --create-only --preview-feature",
    "migrate:dev:pull": "dotenv -e env/local.env -- prisma db pull",
    "migrate:reset:dev": "dotenv -e env/local.env -- prisma migrate reset --preview-feature",
    "migrate:reset:prod": "dotenv -e env/production.env -- prisma migrate reset --preview-feature",
    "migrate:deploy:prod": "dotenv -e env/production.env -- npx prisma migrate deploy --preview-feature",
    "migrate:deploy:dev": "dotenv -e env/dev.env -- npx prisma migrate deploy --preview-feature",
    "migrate:status": "npx prisma migrate status --preview-feature",
    "migrate:resolve": "npx prisma migrate resolve --preview-feature",
    "prisma:studio": "npx prisma studio",
    "prisma:generate": "npx prisma generate",
    "prisma:generate:watch": "npx prisma generate --watch",
    "generate:all": "prisma generate",
    "postinstall": "npm run generate:all"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.879.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^11.1.2",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "^6.6.0",
    "bcryptjs": "^3.0.2",
    "csv-parse": "^6.1.0",
    "dotenv": "^16.5.0",
    "dotenv-cli": "^8.0.0",
    "helmet": "^8.1.0",
    "nanoid": "^5.1.5",
    "nestjs-zod": "^4.3.1",
    "openai": "^5.16.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pdfkit": "^0.17.1",
    "reflect-metadata": "^0.2.0",
    "request-ip": "^3.3.0",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "xlsx": "^0.18.5",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@prisma/internals": "^6.6.0",
    "@prisma/sdk": "^4.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/request-ip": "^0.0.41",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import-helpers": "^2.0.1",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "prisma": "^6.6.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
